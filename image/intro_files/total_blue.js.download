function get_lang_letter_code() {
    switch (document.domain.toString().replace(/^[^\.]+\./, '')) {
        case 'com.br' : return 'pt';
        case 'biz.tr' : return 'tr';
        case 'in.th' : return 'th';
    }
    return document.domain.toString().replace(/^[^\.]+\./, '');
}

